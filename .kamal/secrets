SECRETS=$(kamal secrets fetch --adapter 1password --account $OP_ACCOUNT --from $OP_SECRETS RAILS_MASTER_KEY KAMAL_REGISTRY_PASSWORD INTERNAL_API_SECRET)
RAILS_MASTER_KEY=$(kamal secrets extract RAILS_MASTER_KEY $SECRETS)
KAMAL_REGISTRY_PASSWORD=$(kamal secrets extract KAMAL_REGISTRY_PASSWORD $SECRETS)
INTERNAL_API_SECRET=$(kamal secrets extract INTERNAL_API_SECRET $SECRETS)
